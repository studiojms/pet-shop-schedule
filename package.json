{
  "name": "pet-shop-schedule",
  "version": "1.0.0",
  "description": "A simple pet shop schedule app written in node.js using typescript and graphql",
  "scripts": {
    "dev": "nodemon --watch 'src/' --exec 'ts-node src/server.ts' -e ts",
    "build": "tsc",
    "prod": "npm run build && node dist/server.js",
    "migrate": "sequelize db:migrate",
    "migrate-undo": "sequelize db:migrate:undo:all",
    "migrate-init": "sequelize init"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/studiojms/pet-shop-schedule.git"
  },
  "keywords": [],
  "author": "studiojms",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/studiojms/pet-shop-schedule/issues"
  },
  "homepage": "https://github.com/studiojms/pet-shop-schedule#readme",
  "dependencies": {
    "graphql-yoga": "^1.18.1",
    "mysql2": "^1.6.5",
    "sequelize": "^5.13.0"
  },
  "devDependencies": {
    "@types/sequelize": "^4.28.4",
    "nodemon": "^1.19.1",
    "sequelize-cli": "^5.5.0",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.3"
  }
}
